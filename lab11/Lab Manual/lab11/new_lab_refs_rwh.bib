	Keywords = {ad hoc networks, mobile radio, peer-to-peer computing, telecommunication network reliabilityMANET, centralized network, community mesh network, decentralized wireless network, information theory, mobile ad hoc network, network capacity theory, peer-to-peer network, spatial decomposition, star network, timescale decomposition, unbounded delay},
	Month = {December},
